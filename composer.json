{
    "name": "gmalz/orangemoney-laravel",
    "type": "library",
    "description": "Orange Money web payment Client custom engine.",
    "keywords": ["orangemoney", "orange", "money", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gorissen Malick",
            "email": "gorissenmalick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0@stable",
        "guzzlehttp/guzzle": ">=7.0.1@stable"
    },
    "autoload": {
        "psr-4": {
            "OrangeMoney\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OrangeMoney\\OrangeMoneyServiceProvider"
            ],
            "aliases": {
                "OrangeMoney": "OrangeMoney\\Facades\\OrangeMoneyFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
