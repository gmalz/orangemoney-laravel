{
    "name": "gmalz/orangemoney-laravel",
    "type": "library",
    "description": "Orange Money web payment Client custom engine.",
    "keywords": ["orangemoney", "orange", "money", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gorissen Malick",
            "email": "gorissenmalick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "OrangeMoney\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "OrangeMoney\\OrangeMoneyServiceProvider"
            ],
            "aliases": {
                "OrangeMoney": "OrangeMoney\\Facades\\OrangeMoney"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
